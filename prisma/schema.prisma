generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id                Int     @id @default(autoincrement())
  name              String
  price             Decimal
  brand             String
  category          String
  product_image_url String
  descrption        String
  skin_type         String
  stock_quantity    Int
}
